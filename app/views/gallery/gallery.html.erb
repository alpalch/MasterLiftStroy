            <a href="<%= image_path('gallery/4.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/4s.jpg') %>"></a>
            <a href="<%= image_path('gallery/5.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/5s.jpg') %>"></a>
            <a href="<%= image_path('gallery/6.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/6s.jpg') %>"></a>
            <a href="<%= image_path('gallery/7.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/7s.jpg') %>"></a>
            <a href="<%= image_path('gallery/8.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/8s.jpg') %>"></a>
