<link rel="stylesheet" href="css/blueimp-gallery.min.css">

<div class="wrapper wrapper-content">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">

        <div class="ibox-content">

          <h2>Our projects gallery</h2>

          <div class="lightBoxGallery">
            <a href="<%= image_path('gallery/1.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/1s.jpg') %>"></a>
            <a href="<%= image_path('gallery/2.jpg') %>" title="Image from Unsplash" data-gallery=""><img src="<%= image_path('gallery/2s.jpg') %>"></a>
